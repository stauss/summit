        $(document).ready(function() {

            //Mobile Nav Logic
            var $menuIcon = $('#menuIcon'),
                $gNav = $('.gNav');

            $menuIcon.on('click', function(){

                // if mobile nav is not on
                if(!$gNav.hasClass('on')){
                    $gNav.addClass('on');
                }
                else {
                    $gNav.removeClass('on');
                }

            });


        });

