script(src='assests/js/vendor/swipe.js')
script(src='assests/js/main.js')


script

    $(document).ready(function() {

        $('#nav li').on ('click', function () {
            window.mySwipe.slide($(this).index(), 200);
        });

        $('#mySwipePrev').on('click', function () {
           mySwipe.prev();

           $navLi.removeClass('selected');
           $navLi.eq(mySwipe.getPos() - 1).addClass('selected');
        });
        $('#mySwipeNext').on('click', function () {
           mySwipe.next();
           $navLi.removeClass('selected');
           $navLi.eq(mySwipe.getPos() - 1).addClass('selected');
        });

        // pure JS slider nav
        var elem = document.getElementById('mySwipe');
        window.mySwipe = Swipe(elem, {
          startSlide: 0,
          auto: 1500,
          continuous: true,
          stopPropagation: true,
        });






        //Mobile Nav Logic
        var $menuIcon = $('#menuIcon'),
            $gNav = $('.gNav');

        $menuIcon.on('click', function(){

            // if mobile nav is not on
            if(!$gNav.hasClass('on')){
                $gNav.addClass('on');
            }
            else {
                $gNav.removeClass('on');
            }

        });


    });

